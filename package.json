{
  "name": "@solx/ismediaquery",
  "version": "1.0.0",
  "description": "Utility-function-to-check-if-a-string-is-a-valid-media-query",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\"",
    "build": "rimraf ./dist && tsc",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "jest --collect-coverage",
    "test:ci": "jest --collect-coverage",
    "pretest:cov": "pnpm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "@solx/css-features": "^1.2.1",
    "@solx/css-media-types": "^1.1.1",
    "@solx/css-units": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
